#checks if a list of words is sorted or not.
def issorted(mylist):
    lst=mylist.split()
    #print(lst)
    slist=sorted(lst,key=str.lower)
    #print(slist)
    if lst==slist:
        return True
    else: return False
newlist=input('give me words seperated with space:\n')
print(issorted(newlist))
